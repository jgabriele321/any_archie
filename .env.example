# AnyArchie Configuration
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/anyarchie

# OpenRouter (LLM)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Exa (Web Search)
EXA_API_KEY=your_exa_api_key_here

# Hub Bot Token (the main bot users message to get started)
HUB_BOT_TOKEN=your_hub_bot_token_here

# Bot Token Pool (comma-separated list of tokens for user bots)
# Create these via @BotFather on Telegram
BOT_TOKEN_POOL=token1,token2,token3,token4,token5

# Admin Telegram ID (your personal Telegram user ID for admin commands)
ADMIN_TELEGRAM_ID=your_telegram_id_here
